{
    "name": "ruhaycreative/gram",
    "type": "project",
    "description": "The Laravel Framework boilerplate with Tailwind CSS scaffolding.",
    "keywords": [
        "laravel",
        "framework",
        "boilerplate",
        "scaffolding",
        "tailwind"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.1",
        "appstract/laravel-options": "^2.2",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "garygreen/pretty-routes": "^1.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.2",
        "laravel/socialite": "^4.1",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "pragmarx/version": "^0.2.8",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^1.1",
        "renatomarinho/laravel-page-speed": "^1.8",
        "robinvdvleuten/ulid": "^3.0",
        "spatie/laravel-permission": "^2.36",
        "theiconic/name-parser": "^1.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "spatie/laravel-web-tinker": "^1.3",
        "wnx/laravel-stats": "^1.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
